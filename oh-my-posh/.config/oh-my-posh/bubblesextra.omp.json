{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "options": {
            "style": "folder"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\uf07b {{ .Path }}",
          "foreground": "#89b4fa",
          "background": "#313244",
          "type": "path",
          "style": "diamond"
        },
        {
          "options": {
            "branch_template": "{{ trunc 25 .Branch }}",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}",
          "foreground": "#cba6f7",
          "background": "#313244",
          "type": "git",
          "style": "diamond"
        },
        {
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "{{ if and .Env.OMP_IS_GIT_REPO (and .Env.OMP_MULTI_LANG_DETECTED (not .Env.OMP_SHOW_ALL_LANGS)) }}üíÄ{{ end }}",
          "foreground": "#11111b",
          "background": "#f38ba8",
          "type": "text",
          "style": "diamond"
        },
        {
          "options": {
            "display_mode": "files",
            "fetch_version": true,
            "fetch_virtual_env": true
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "{{ if and .Env.OMP_IS_GIT_REPO (or (not .Env.OMP_MULTI_LANG_DETECTED) .Env.OMP_SHOW_ALL_LANGS) }}{{ if .Venv }}{{ .Venv }}{{ else }}Python{{ end }}({{ .Full }}){{ end }}",
          "foreground": "#fab387",
          "background": "#313244",
          "type": "python",
          "style": "diamond"
        },
        {
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "{{ if and .Env.OMP_IS_GIT_REPO (and .Env.OMP_MULTI_LANG_DETECTED (and .Env.OMP_PYTHON_DEEP .Env.OMP_SHOW_ALL_LANGS)) }}Python{{ end }}",
          "foreground": "#fab387",
          "background": "#313244",
          "type": "text",
          "style": "diamond"
        },
        {
          "options": {
            "display_mode": "files",
            "fetch_version": true
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "{{ if and .Env.OMP_IS_GIT_REPO (or (not .Env.OMP_MULTI_LANG_DETECTED) .Env.OMP_SHOW_ALL_LANGS) }}Go({{ .Full }}){{ end }}",
          "foreground": "#74c7ec",
          "background": "#313244",
          "type": "go",
          "style": "diamond"
        },
        {
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "{{ if and .Env.OMP_IS_GIT_REPO (and .Env.OMP_MULTI_LANG_DETECTED (and .Env.OMP_GO_DEEP .Env.OMP_SHOW_ALL_LANGS)) }}Go{{ end }}",
          "foreground": "#74c7ec",
          "background": "#313244",
          "type": "text",
          "style": "diamond"
        },
        {
          "options": {
            "display_mode": "files",
            "fetch_version": true
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "{{ if and .Env.OMP_IS_GIT_REPO (or (not .Env.OMP_MULTI_LANG_DETECTED) .Env.OMP_SHOW_ALL_LANGS) }}Lua({{ .Full }}){{ end }}",
          "foreground": "#94e2d5",
          "background": "#313244",
          "type": "lua",
          "style": "diamond"
        },
        {
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "{{ if and .Env.OMP_IS_GIT_REPO (and .Env.OMP_MULTI_LANG_DETECTED (and .Env.OMP_LUA_DEEP .Env.OMP_SHOW_ALL_LANGS)) }}Lua{{ end }}",
          "foreground": "#94e2d5",
          "background": "#313244",
          "type": "text",
          "style": "diamond"
        },
        {
          "options": {
            "display_mode": "files",
            "fetch_version": true
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "{{ if and .Env.OMP_IS_GIT_REPO (or (not .Env.OMP_MULTI_LANG_DETECTED) .Env.OMP_SHOW_ALL_LANGS) }}TS({{ .Full }}){{ end }}",
          "foreground": "#a6e3a1",
          "background": "#313244",
          "type": "node",
          "style": "diamond"
        },
        {
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "{{ if and .Env.OMP_IS_GIT_REPO (and .Env.OMP_MULTI_LANG_DETECTED (and .Env.OMP_NODE_DEEP .Env.OMP_SHOW_ALL_LANGS)) }}TS{{ end }}",
          "foreground": "#a6e3a1",
          "background": "#313244",
          "type": "text",
          "style": "diamond"
        },
        {
          "options": {
            "display_mode": "files",
            "fetch_version": true
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "{{ if and .Env.OMP_IS_GIT_REPO (or (not .Env.OMP_MULTI_LANG_DETECTED) .Env.OMP_SHOW_ALL_LANGS) }}Rust({{ .Full }}){{ end }}",
          "foreground": "#eba0ac",
          "background": "#313244",
          "type": "rust",
          "style": "diamond"
        },
        {
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "{{ if and .Env.OMP_IS_GIT_REPO (and .Env.OMP_MULTI_LANG_DETECTED (and .Env.OMP_RUST_DEEP .Env.OMP_SHOW_ALL_LANGS)) }}Rust{{ end }}",
          "foreground": "#eba0ac",
          "background": "#313244",
          "type": "text",
          "style": "diamond"
        },
        {
          "template": " {{ if gt .Code 0 }}\uf00d{{ else }}\uf00c{{ end }}({{ .FormattedMs }})",
          "foreground": "#cdd6f4",
          "background": "transparent",
          "type": "executiontime",
          "style": "plain",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#f38ba8{{ end }}",
            "{{ if eq .Code 0 }}#a6e3a1{{ end }}"
          ]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "{{ if .SSHSession }}{{ .HostName }}{{ else }}{{ .UserName }}{{ end }} ‚ùØ",
          "foreground": "#cdd6f4",
          "background": "#313244",
          "type": "session",
          "style": "diamond"
        }
      ]
    }
  ],
  "version": 4,
  "final_space": true
}